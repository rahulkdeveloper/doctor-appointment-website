                    <div class="card-body text-center">
                        <h2 class="card-title">I am Patient</h2>
                        <a href="/login" class="btn btn-primary">Login here</a>
                    </div>
                </div>

                <div class="card patient-card" style="width: 18rem;">
                    <img class="card-img-top" src="../public/assets/img/patients/patient2.jpg" alt="Card image cap">
                    <div class="card-body text-center">
                        <h2 class="card-title">I am Docter</h2>
                        <a href="/doctor-login" class="btn btn-primary">Login here</a>
                    </div>
                </div>
            </div>

        </div>
